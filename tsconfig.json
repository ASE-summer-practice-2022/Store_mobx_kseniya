{



  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/*": ["../src/*"],
      "@pages/*": ["../src/pages/*"],
      "@styles/*": ["../src/styles/*"],
      "@services/*": ["../src/services/*"],
      "@models/*": ["../src/models/*"],
      "@store/*": ["../src/store/*"],
      "@router/*": ["../src/router/*"],

    },
    "outDir": "build/dist",
    "resolveJsonModule": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "allowJs": true,
    "moduleResolution": "node",
    "rootDir": "src",
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  },
  "include": ["./src/**/*.tsx", "./src/**/*.ts", "./Globals/*.ts"],
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "src/setupTests.ts"
  ],
  "types": [
    "typePatches"
  ]
}